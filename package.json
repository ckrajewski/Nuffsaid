    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.5.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.8.6"
  },
  "devDependencies": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
